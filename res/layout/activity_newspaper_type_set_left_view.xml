<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/setLeftMainLayoutView"
        android:layout_width="230.0dip"
        android:layout_height="220.0dip"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="#ff000000" >

        <RelativeLayout
            android:id="@+id/setLeftPhotoView"
            android:layout_width="174.0dip"
            android:layout_height="194.0dip"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="14.0dip"
            android:layout_marginTop="14.0dip"
            android:background="@drawable/newspaper_layout_style" >

            <ImageView
                android:id="@+id/setLeftEditPhotoIconView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:src="@drawable/icon_img" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="20.0dip"
            android:layout_height="194.0dip"
            android:layout_alignBottom="@+id/setLeftPhotoView"
            android:layout_marginLeft="12.0dip"
            android:layout_toRightOf="@+id/setLeftPhotoView"
            android:background="@drawable/newspaper_layout_style" >

            <ImageView
                android:id="@+id/setLeftEditTextIconView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:src="@drawable/icon_edit" />

            <TextView
                android:id="@+id/setLeftEditTextView"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:gravity="center"
                android:textColor="#ffffffff"
                android:textSize="16.0sp" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="fill_parent"
        android:layout_height="200.0dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true" >

        <ImageView
            android:id="@+id/ImageView01"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/dotted_line1" />

        <Button
            android:id="@+id/setButtonConfirmView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ImageView01"
            android:layout_marginTop="35.0dip"
            android:layout_toRightOf="@+id/setButtonBackView"
            android:background="@drawable/btn_sure_selector" />

        <Button
            android:id="@+id/setButtonBackView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/setButtonConfirmView"
            android:layout_alignBottom="@+id/setButtonConfirmView"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="71.0dip"
            android:background="@drawable/btn_return_selector" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="-3.0dip"
        android:layout_toRightOf="@+id/setLeftMainLayoutView"
        android:src="@drawable/dotted_line2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/setLeftMainLayoutView"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="-5.0dip"
        android:src="@drawable/dotted_line2" />

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/header_bg2" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/title_edit_illustrate" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout10"
        android:layout_width="wrap_content"
        android:layout_height="106.0dip"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/relativeLayout1" >

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/imageView3"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="29.0dip"
            android:src="@drawable/handwrite_07" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:src="@drawable/dotted_line1" />
    </RelativeLayout>

</RelativeLayout>